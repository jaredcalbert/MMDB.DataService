@model IEnumerable<MMDB.DataService.Data.Metadata.DataObjectView>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr class="ui-widget-header">
        <th>
            ObjectTypeName
        </th>
		<th>
			ViewName
		</th>
        <th>
            ResourceAssemblyName
        </th>
        <th>
            ResourceIdentifier
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ViewName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResourceAssemblyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResourceIdentifier)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
