@model MMDB.DataService.Data.Metadata.CustomControllerMetadata

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Delete Custom Controller</legend>

    <div class="display-label">Controller Name:</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ControllerName)
    </div>

    <div class="display-label">Class Name:</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ClassName)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
